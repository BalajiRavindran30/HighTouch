{
  "name": "vimeo-sfmc",
  "private": true,
  "workspaces": {
    "packages": [
      "app/*"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start:api": "npm --prefix app/api start",
    "start:ui": "npm --prefix app/ui start",
    "build:ui": "npm --prefix app/ui run build",
    "start": "run-p build:ui start:api"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "gzip": "^0.1.0",
    "openssl": "^2.0.0",
    "react-scripts": "^5.0.1",
    "webpack-dev-middleware": "^5.3.3"
  },
  "description": "This repo contains the code for an app that can be embedded into SFMC via their AppExchange partner program.\r In technical terms, this app is loaded via an `iframe` under the `https://*.exacttarget.com` domain.",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "yarn@1.22.19"
}
